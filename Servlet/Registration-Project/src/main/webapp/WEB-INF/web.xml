<?xml version="1.0" encoding="UTF-8"?>

<web-app>
	
	<servlet>
		<servlet-name>SIGNUP</servlet-name>
		<servlet-class>com.Servlet.SignUpServlet</servlet-class>
	</servlet>

	
	<servlet-mapping>
		<servlet-name>SIGNUP</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LOGIN</servlet-name>
		<servlet-class>com.Servlet.LoginServlet</servlet-class>
	</servlet>

	
	<servlet-mapping>
		<servlet-name>LOGIN</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>COLLEAGUES</servlet-name>
		<servlet-class>com.Servlet.ColleaguesServlet</servlet-class>
	</servlet>

	
	<servlet-mapping>
		<servlet-name>COLLEAGUES</servlet-name>
		<url-pattern>/colleagues</url-pattern>
	</servlet-mapping>
	
	
	
</web-app>